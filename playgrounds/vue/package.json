{
    "name": "@codemonster-ru/ssr-service-playground",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build:client": "vite build -c vite.config.client.ts",
        "build:server": "vite build -c vite.config.server.ts",
        "build": "npm run build:client && npm run build:server",
        "serve:ssr": "node ../../dist/bin/cli.js server --serverEntry ./dist/server/entry-server.js --port 3001"
    },
    "dependencies": {
        "vue": "^3.5.0"
    },
    "devDependencies": {
        "vue": "^3.5.21",
        "@vitejs/plugin-vue": "^6.0.1",
        "typescript": "^5.0.0"
    }
}
